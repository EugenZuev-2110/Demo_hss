@model WebProject.Areas.TSO.Models.TZOtherOrgCostsViewModel
@{
	Layout = null; string disable = "";
}
@if (Model.tz_id == 0)
{
	disable = "disabled";
}
<form asp-controller="CostsAndPrices" asp-action="TZOtherOrgCosts_Save" asp-antiforgery="true" asp-area="TSO" id="tz_other_org_costs_form"
	  data-ajax-failure="onFailedFormTZ" data-ajax-success="onSuccessFormTZ" data-ajax-begin="onBeginFormTZ" data-ajax="true" data-ajax-method="POST">
	<input type="hidden" name="tz_id" value="@Model.tz_id" />
	<input type="hidden" name="data_status" value="@Model.data_status" />
	<input type="hidden" name="perspective_year" value="@Model.perspective_year" />
	<table class="table-inp-stretch" style="width:100%;">
		<thead>
			<tr>
				<th rowspan="2"></th>
				<th colspan="3">Факт</th>
				<th colspan="3">План</th>
			</tr>
			<tr>
				<th>I полугодие</th>
				<th>II полугодие</th>
				<th>Год</th>
				<th>I полугодие</th>
				<th>II полугодие</th>
				<th>Год</th>
			</tr>
		</thead>
		<tbody>
			<tr>
				<td>Всего расходов, <span class="data-unit">тыс. руб.</span></td>
				<td><input type="text" asp-for="@Model.cost_other_org_all_fact_1" class="data-value" disabled></td>
				<td><input type="text" asp-for="@Model.cost_other_org_all_fact_2" class="data-value" disabled></td>
				<td><input type="text" asp-for="@Model.cost_other_org_all_fact_3" class="data-value" disabled></td>
				<td><input type="text" asp-for="@Model.cost_other_org_all_plan_1" class="data-value" disabled></td>
				<td><input type="text" asp-for="@Model.cost_other_org_all_plan_2" class="data-value" disabled></td>
				<td><input type="text" asp-for="@Model.cost_other_org_all_plan_3" class="data-value" disabled></td>
			</tr>
			<tr>
				<td>1. Расходы на транспортные услуги, <span class="data-unit">тыс. руб.</span></td>
				<td><input type="text" asp-for="@Model.cost_transport_fact_1" class="data-value double" data-double="other_org_costs"></td>
				<td><input type="text" asp-for="@Model.cost_transport_fact_2" class="data-value double" data-double="other_org_costs"></td>
				<td><input type="text" asp-for="@Model.cost_transport_fact_3" class="data-value" disabled></td>
				<td><input type="text" asp-for="@Model.cost_transport_plan_1" class="data-value double" data-double="other_org_costs"></td>
				<td><input type="text" asp-for="@Model.cost_transport_plan_2" class="data-value double" data-double="other_org_costs"></td>
				<td><input type="text" asp-for="@Model.cost_transport_plan_3" class="data-value" disabled></td>
			</tr>
			<tr>
				<td>2. Расходы на работы по техническому регламенту, <span class="data-unit">тыс. руб.</span></td>
				<td><input type="text" asp-for="@Model.cost_tech_regulation_fact_1" class="data-value double" data-double="other_org_costs"></td>
				<td><input type="text" asp-for="@Model.cost_tech_regulation_fact_2" class="data-value double" data-double="other_org_costs"></td>
				<td><input type="text" asp-for="@Model.cost_tech_regulation_fact_3" class="data-value" disabled></td>
				<td><input type="text" asp-for="@Model.cost_tech_regulation_plan_1" class="data-value double" data-double="other_org_costs"></td>
				<td><input type="text" asp-for="@Model.cost_tech_regulation_plan_2" class="data-value double" data-double="other_org_costs"></td>
				<td><input type="text" asp-for="@Model.cost_tech_regulation_plan_3" class="data-value" disabled></td>
			</tr>
			<tr>
				<td>3. Расходы на прочие услуги вспомогательных производств, <span class="data-unit">тыс. руб.</span></td>
				<td><input type="text" asp-for="@Model.cost_other_support_prod_fact_1" class="data-value double" data-double="other_org_costs"></td>
				<td><input type="text" asp-for="@Model.cost_other_support_prod_fact_2" class="data-value double" data-double="other_org_costs"></td>
				<td><input type="text" asp-for="@Model.cost_other_support_prod_fact_3" class="data-value" disabled></td>
				<td><input type="text" asp-for="@Model.cost_other_support_prod_plan_1" class="data-value double" data-double="other_org_costs"></td>
				<td><input type="text" asp-for="@Model.cost_other_support_prod_plan_2" class="data-value double" data-double="other_org_costs"></td>
				<td><input type="text" asp-for="@Model.cost_other_support_prod_plan_3" class="data-value" disabled></td>
			</tr>
			<tr>
				<td>4. Расходы на иные работы и услуги производственного характера, <span class="data-unit">тыс. руб.</span></td>
				<td><input type="text" asp-for="@Model.cost_other_service_prod_fact_1" class="data-value double" data-double="other_org_costs"></td>
				<td><input type="text" asp-for="@Model.cost_other_service_prod_fact_2" class="data-value double" data-double="other_org_costs"></td>
				<td><input type="text" asp-for="@Model.cost_other_service_prod_fact_3" class="data-value" disabled></td>
				<td><input type="text" asp-for="@Model.cost_other_service_prod_plan_1" class="data-value double" data-double="other_org_costs"></td>
				<td><input type="text" asp-for="@Model.cost_other_service_prod_plan_2" class="data-value double" data-double="other_org_costs"></td>
				<td><input type="text" asp-for="@Model.cost_other_service_prod_plan_3" class="data-value" disabled></td>
			</tr>
		</tbody>
	</table>
	<button type="submit" class="bttn bttn-color ms-auto" @disable>Сохранить</button>
</form>