@model WebProject.Areas.TSO.Models.TZUncontrolOtherCostsViewModel
@{
	Layout = null; string disable = "";
}
@if (Model.tz_id == 0)
{
	disable = "disabled";
}
<form asp-controller="CostsAndPrices" asp-action="TZUncontrolOtherCosts_Save" asp-antiforgery="true" asp-area="TSO"
	  data-ajax-failure="onFailedFormTZ" data-ajax-success="onSuccessFormTZ" data-ajax-begin="onBeginFormTZ" data-ajax="true" data-ajax-method="POST">
	<input type="hidden" name="tz_id" value="@Model.tz_id" />
	<input type="hidden" name="data_status" value="@Model.data_status" />
	<input type="hidden" name="perspective_year" value="@Model.perspective_year" />
	<table class="table-inp-stretch" style="width:100%;">
		<thead>
			<tr>
				<th rowspan="2"></th>
				<th colspan="3">Факт</th>
				<th colspan="3">План</th>
			</tr>
			<tr>
				<th style="position: static;">I полугодие</th>
				<th>II полугодие</th>
				<th>Год</th>
				<th>I полугодие</th>
				<th>II полугодие</th>
				<th>Год</th>
			</tr>
		</thead>
		<tbody>
			<tr>
				<td>Концессионная плата, <span class="data-unit">тыс. руб.</span></td>
				<td><input type="text" asp-for="@Model.cost_concession_fact_1" class="data-value double" data-double="uncontrol_other_costs"></td>
				<td><input type="text" asp-for="@Model.cost_concession_fact_2" class="data-value double" data-double="uncontrol_other_costs"></td>
				<td><input type="text" asp-for="@Model.cost_concession_fact_3" class="data-value" disabled></td>
				<td><input type="text" asp-for="@Model.cost_concession_plan_1" class="data-value double" data-double="uncontrol_other_costs"></td>
				<td><input type="text" asp-for="@Model.cost_concession_plan_2" class="data-value double" data-double="uncontrol_other_costs"></td>
				<td><input type="text" asp-for="@Model.cost_concession_plan_3" class="data-value" disabled></td>
			</tr>
			<tr>
				<td>Расходы по сомнительным долгам, <span class="data-unit">тыс. руб.</span></td>
				<td><input type="text" asp-for="@Model.cost_doubtful_debt_fact_1" class="data-value double" data-double="uncontrol_other_costs"></td>
				<td><input type="text" asp-for="@Model.cost_doubtful_debt_fact_2" class="data-value double" data-double="uncontrol_other_costs"></td>
				<td><input type="text" asp-for="@Model.cost_doubtful_debt_fact_3" class="data-value" disabled></td>
				<td><input type="text" asp-for="@Model.cost_doubtful_debt_plan_1" class="data-value double" data-double="uncontrol_other_costs"></td>
				<td><input type="text" asp-for="@Model.cost_doubtful_debt_plan_2" class="data-value double" data-double="uncontrol_other_costs"></td>
				<td><input type="text" asp-for="@Model.cost_doubtful_debt_plan_3" class="data-value" disabled></td>
			</tr>
			<tr>
				<td>Расходы на выплаты по договорам займа и кредитным договорам,<br>включая проценты по ним, <span class="data-unit">тыс. руб.</span></td>
				<td><input type="text" asp-for="@Model.cost_credit_contracts_fact_1" class="data-value double" data-double="uncontrol_other_costs"></td>
				<td><input type="text" asp-for="@Model.cost_credit_contracts_fact_2" class="data-value double" data-double="uncontrol_other_costs"></td>
				<td><input type="text" asp-for="@Model.cost_credit_contracts_fact_3" class="data-value" disabled></td>
				<td><input type="text" asp-for="@Model.cost_credit_contracts_plan_1" class="data-value double" data-double="uncontrol_other_costs"></td>
				<td><input type="text" asp-for="@Model.cost_credit_contracts_plan_2" class="data-value double" data-double="uncontrol_other_costs"></td>
				<td><input type="text" asp-for="@Model.cost_credit_contracts_plan_3" class="data-value" disabled></td>
			</tr>
			<tr>
				<td>Арендная плата производственных объектов, <span class="data-unit">тыс. руб.</span></td>
				<td><input type="text" asp-for="@Model.rent_prod_obj_all_fact_1" class="data-value" disabled></td>
				<td><input type="text" asp-for="@Model.rent_prod_obj_all_fact_2" class="data-value" disabled></td>
				<td><input type="text" asp-for="@Model.rent_prod_obj_all_fact_3" class="data-value" disabled></td>
				<td><input type="text" asp-for="@Model.rent_prod_obj_all_plan_1" class="data-value" disabled></td>
				<td><input type="text" asp-for="@Model.rent_prod_obj_all_plan_2" class="data-value" disabled></td>
				<td><input type="text" asp-for="@Model.rent_prod_obj_all_plan_3" class="data-value" disabled></td>
			</tr>
			<tr>
				<td>- в государственной собственности</td>
				<td><input type="text" asp-for="@Model.rent_prod_obj_state_prop_fact_1" class="data-value double" data-double="uncontrol_other_costs"></td>
				<td><input type="text" asp-for="@Model.rent_prod_obj_state_prop_fact_2" class="data-value double" data-double="uncontrol_other_costs"></td>
				<td><input type="text" asp-for="@Model.rent_prod_obj_state_prop_fact_3" class="data-value" disabled></td>
				<td><input type="text" asp-for="@Model.rent_prod_obj_state_prop_plan_1" class="data-value double" data-double="uncontrol_other_costs"></td>
				<td><input type="text" asp-for="@Model.rent_prod_obj_state_prop_plan_2" class="data-value double" data-double="uncontrol_other_costs"></td>
				<td><input type="text" asp-for="@Model.rent_prod_obj_state_prop_plan_3" class="data-value" disabled></td>
			</tr>
			<tr>
				<td>- в муниципальной собственности</td>
				<td><input type="text" asp-for="@Model.rent_prod_obj_municipal_prop_fact_1" class="data-value double" data-double="uncontrol_other_costs"></td>
				<td><input type="text" asp-for="@Model.rent_prod_obj_municipal_prop_fact_2" class="data-value double" data-double="uncontrol_other_costs"></td>
				<td><input type="text" asp-for="@Model.rent_prod_obj_municipal_prop_fact_3" class="data-value" disabled></td>
				<td><input type="text" asp-for="@Model.rent_prod_obj_municipal_prop_plan_1" class="data-value double" data-double="uncontrol_other_costs"></td>
				<td><input type="text" asp-for="@Model.rent_prod_obj_municipal_prop_plan_2" class="data-value double" data-double="uncontrol_other_costs"></td>
				<td><input type="text" asp-for="@Model.rent_prod_obj_municipal_prop_plan_3" class="data-value" disabled></td>
			</tr>
			<tr>
				<td>- прочие</td>
				<td><input type="text" asp-for="@Model.rent_prod_obj_other_fact_1" class="data-value double" data-double="uncontrol_other_costs"></td>
				<td><input type="text" asp-for="@Model.rent_prod_obj_other_fact_2" class="data-value double" data-double="uncontrol_other_costs"></td>
				<td><input type="text" asp-for="@Model.rent_prod_obj_other_fact_3" class="data-value" disabled></td>
				<td><input type="text" asp-for="@Model.rent_prod_obj_other_plan_1" class="data-value double" data-double="uncontrol_other_costs"></td>
				<td><input type="text" asp-for="@Model.rent_prod_obj_other_plan_2" class="data-value double" data-double="uncontrol_other_costs"></td>
				<td><input type="text" asp-for="@Model.rent_prod_obj_other_plan_3" class="data-value" disabled></td>
			</tr>
			<tr>
				<td>Лизинговый платеж, <span class="data-unit">тыс. руб.</span></td>
				<td><input type="text" asp-for="@Model.leasing_obj_all_fact_1" class="data-value" disabled></td>
				<td><input type="text" asp-for="@Model.leasing_obj_all_fact_2" class="data-value" disabled></td>
				<td><input type="text" asp-for="@Model.leasing_obj_all_fact_3" class="data-value" disabled></td>
				<td><input type="text" asp-for="@Model.leasing_obj_all_plan_1" class="data-value" disabled></td>
				<td><input type="text" asp-for="@Model.leasing_obj_all_plan_2" class="data-value" disabled></td>
				<td><input type="text" asp-for="@Model.leasing_obj_all_plan_3" class="data-value" disabled></td>
			</tr>
			<tr>
				<td>- производственных объектов</td>
				<td><input type="text" asp-for="@Model.leasing_prod_obj_fact_1" class="data-value double" data-double="uncontrol_other_costs"></td>
				<td><input type="text" asp-for="@Model.leasing_prod_obj_fact_2" class="data-value double" data-double="uncontrol_other_costs"></td>
				<td><input type="text" asp-for="@Model.leasing_prod_obj_fact_3" class="data-value" disabled></td>
				<td><input type="text" asp-for="@Model.leasing_prod_obj_plan_1" class="data-value double" data-double="uncontrol_other_costs"></td>
				<td><input type="text" asp-for="@Model.leasing_prod_obj_plan_2" class="data-value double" data-double="uncontrol_other_costs"></td>
				<td><input type="text" asp-for="@Model.leasing_prod_obj_plan_3" class="data-value" disabled></td>
			</tr>
			<tr>
				<td>- непроизводственных объектов (с переходом права собственности)</td>
				<td><input type="text" asp-for="@Model.leasing_nonprod_obj_with_own_rights_fact_1" class="data-value double" data-double="uncontrol_other_costs"></td>
				<td><input type="text" asp-for="@Model.leasing_nonprod_obj_with_own_rights_fact_2" class="data-value double" data-double="uncontrol_other_costs"></td>
				<td><input type="text" asp-for="@Model.leasing_nonprod_obj_with_own_rights_fact_3" class="data-value" disabled></td>
				<td><input type="text" asp-for="@Model.leasing_nonprod_obj_with_own_rights_plan_1" class="data-value double" data-double="uncontrol_other_costs"></td>
				<td><input type="text" asp-for="@Model.leasing_nonprod_obj_with_own_rights_plan_2" class="data-value double" data-double="uncontrol_other_costs"></td>
				<td><input type="text" asp-for="@Model.leasing_nonprod_obj_with_own_rights_plan_3" class="data-value" disabled></td>
			</tr>
			<tr>
				<td>Прочие неподконтрольные расходы, <span class="data-unit">тыс. руб.</span></td>
				<td><input type="text" asp-for="@Model.cost_uncontrol_other_fact_1" class="data-value double" data-double="uncontrol_other_costs"></td>
				<td><input type="text" asp-for="@Model.cost_uncontrol_other_fact_2" class="data-value double" data-double="uncontrol_other_costs"></td>
				<td><input type="text" asp-for="@Model.cost_uncontrol_other_fact_3" class="data-value" disabled></td>
				<td><input type="text" asp-for="@Model.cost_uncontrol_other_plan_1" class="data-value double" data-double="uncontrol_other_costs"></td>
				<td><input type="text" asp-for="@Model.cost_uncontrol_other_plan_2" class="data-value double" data-double="uncontrol_other_costs"></td>
				<td><input type="text" asp-for="@Model.cost_uncontrol_other_plan_3" class="data-value" disabled></td>
			</tr>
		</tbody>
	</table>
	<button type="submit" class="bttn bttn-color ms-auto" @disable>Сохранить</button>
</form>