@model WebProject.Areas.HPConsumers.Models.Consumers_SubPartialYearDynamicViewModel

@{
    Layout = null; string is_disabled = ViewBag.IsDisabled;
}


<input hidden name="consumer_id" value="@Model.Consumers_PerspectiveDev_SubPartialViewModel.consumer_id">
<input hidden name="data_status" value="@Model.Consumers_PerspectiveDev_SubPartialViewModel.data_status">
<input hidden name="dev_prog_id" value="@Model.Consumers_PerspectiveDev_SubPartialViewModel.dev_prog_id">
<div class="data-block">
    <div class="data-row">
        <div class="col-12 col-xl-5">
            <div class="data-block me-xl-3">
                <div class="data-item">
                    <label class="data-label lg-fix">Программа развития:</label>
                    <input type="text" class="data-value stretch" name="type_prog_name" value="@Model.Consumers_PerspectiveDev_SubPartialViewModel.type_prog_name" id="type_prog_name"
                           placeholder="Наименование типа программы" disabled>
                </div>
                <div class="data-item">
                    <label class="data-label lg-fix">Уникальный номер объекта по программе:</label>
                    <input type="text" class="data-value stretch" name="obj_num" value="@Model.Consumers_PerspectiveDev_SubPartialViewModel.obj_num" id="obj_num"
                           placeholder="Номер объекта в программе" disabled>
                </div>
                <div class="data-row">
                    <div class="data-item ">
                        <label class="data-label lg-fix">Заявка о подключении: </label><label class="data-label sublabel">от</label>
                        <input type="date" class="data-value smx-stretch" name="connect_req_date" value="@Model.Consumers_PerspectiveDev_SubPartialViewModel.connect_req_date" id="connect_req_date"
                               placeholder="Дата" disabled>
                    </div>
                    <div class="data-item ">
                        <label class="data-label sublabel">№</label>
                        <input type="text" class="data-value smx-stretch" name="connect_req_num" value="@Model.Consumers_PerspectiveDev_SubPartialViewModel.connect_req_num" id="connect_req_num"
                               placeholder="Номер" disabled>
                    </div>
                </div>
                <div class="data-row">
                    <div class="data-item ">
                        <label class="data-label lg-fix">Договор о подключении: </label><label class="data-label sublabel">от</label>
                        <input type="date" class="data-value smx-stretch" name="connect_agree_date" value="@Model.Consumers_PerspectiveDev_SubPartialViewModel.connect_agree_date" id="connect_agree_date"
                               placeholder="Дата" disabled>
                    </div>
                    <div class="data-item ">
                        <label class="data-label sublabel">№</label>
                        <input type="text" class="data-value smx-stretch" name="connect_agree_num" value="@Model.Consumers_PerspectiveDev_SubPartialViewModel.connect_agree_num" id="connect_agree_num"
                               placeholder="Номер" disabled>
                    </div>
                </div>
                <div class="data-item">
                    <label class="data-label lg-fix">Общий объем строительства:</label>
                    <input type="text" class="data-value smx-stretch" name="area" value="@Model.Consumers_PerspectiveDev_SubPartialViewModel.area" id="area"
                           placeholder="Общая площадь" disabled>
                </div>
            </div>
        </div>
        <div class="col-12 col-xl-7">
            <div class="data-block ms-xl-3">
                <div class="data-row">
                    <div class="data-item ">
                        <label class="data-label lg-fix">Год программы:</label>
                        <input type="text" class="data-value smx-stretch" name="year_dev_prog" value="@Model.Consumers_PerspectiveDev_SubPartialViewModel.year_dev_prog" id="year_dev_prog"
                               placeholder="Год" disabled>
                    </div>
                    <div class="data-row stretch">
                        <div class="data-item ">
                            <label class="data-label lg-fix">Решение: </label><label class="data-label sublabel">от</label>
                            <input type="date" class="data-value md-fix" name="date_dev_prog" value="@Model.Consumers_PerspectiveDev_SubPartialViewModel.date_dev_prog" id="date_dev_prog"
                                   placeholder="Дата" disabled>
                        </div>
                        <div class="data-item ">
                            <label class="data-label sublabel">№</label>
                            <input type="text" class="data-value sm-stretch" name="prog_doc_num" value="@Model.Consumers_PerspectiveDev_SubPartialViewModel.prog_doc_num" id="prog_doc_num"
                                   placeholder="Номер" disabled>
                        </div>
                    </div>
                </div>
                <div class="data-item">
                    <label class="data-label lg-fix">Наименование заявителя:</label>
                    <input type="text" class="data-value xl-stretch" name="appl_name" value="@Model.Consumers_PerspectiveDev_SubPartialViewModel.appl_name" id="appl_name"
                           placeholder="Наименование заявителя по объекту" disabled>
                </div>
                <div class="data-row">
                    <div class="data-item">
                        <label class="data-label lg-fix">Условие на подключение: </label><label class="data-label sublabel">от</label>
                        <input type="date" class="data-value md-fix" name="connect_cond_date" value="@Model.Consumers_PerspectiveDev_SubPartialViewModel.connect_cond_date" id="connect_cond_date"
                               placeholder="Дата" disabled>
                    </div>
                    <div class="data-item ">
                        <label class="data-label sublabel">№</label>
                        <input type="text" class="data-value sm-stretch" name="connect_cond_num" value="@Model.Consumers_PerspectiveDev_SubPartialViewModel.connect_cond_num" id="connect_cond_num"
                               placeholder="Номер" disabled>
                    </div>
                </div>
                <div class="data-item">
                    <label class="data-label lg-fix">Точка присоединения:</label>
                    <input type="text" class="data-value stretch" name="connect_point_hn" value="@Model.Consumers_PerspectiveDev_SubPartialViewModel.connect_point_hn" id="connect_point_hn"
                           placeholder="Наименование точки на присоединение" disabled>
                </div>
                <div class="data-row">
                    <div class="data-item ">
                        <label class="data-label lg-fix">Года реализации строительства: </label><label class="data-label sublabel">год начала</label>
                        <input type="text" class="data-value smx-stretch" name="start_year" value="@Model.Consumers_PerspectiveDev_SubPartialViewModel.start_year" id="start_year"
                               placeholder="Год" disabled>
                    </div>
                    <div class="data-item ">
                        <label class="data-label lg-fix sublabel">год окончания</label>
                        <input type="text" class="data-value smx-stretch" name="end_year" value="@Model.Consumers_PerspectiveDev_SubPartialViewModel.end_year" id="end_year"
                               placeholder="Год" disabled>
                    </div>
                </div>
            </div>
        </div>
