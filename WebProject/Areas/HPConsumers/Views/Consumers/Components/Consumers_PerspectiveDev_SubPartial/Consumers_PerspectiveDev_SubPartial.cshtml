                        }
                    </tr>
                    <tr>
                        <td>Суммарная расчетная<br>тепловая нагрузка</td>
                        <td colspan="@Model.Consumers_PerspectiveDev_YearDynamicViewModel.Count()"></td>
                    </tr>
                    <tr>
                        <td>- горячая вода, <span class="data-unit">Гкал/ч</span></td>
                        @foreach (var p_year in Model.Consumers_PerspectiveDev_YearDynamicViewModel)
                        {
                            <th><input type="text" class="data-value" name="p_year_calc_sum_hw" value="@p_year.calc_sum_hw" id="p_year_calc_sum_hw" disabled></th>
                        }
                    </tr>
                    <tr>
                        <td>- пар, <span class="data-unit">т/ч</span></td>
                        @foreach (var p_year in Model.Consumers_PerspectiveDev_YearDynamicViewModel)
                        {
                            <th><input type="text" class="data-value" name="p_year_calc_sum_steam" value="@p_year.calc_sum_steam" id="p_year_calc_sum_steam" disabled></th>
                        }
                    </tr>
                </tbody>
            </table>
        </div>
    </div>
</div>

