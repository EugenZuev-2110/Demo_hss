@model List<WebProject.Areas.DictionaryTables.Models.CalcConsumptionGVSViewModel>

<h4>В) Укрупнённые удельные расходы тепла на нужды горячего водоснабжения (среднечасовое значение)</h4>
<table id="consumption2Table" class="display compact table-inp-stretch" style="width:100%;">
    <thead>
        <tr>
            <th rowspan="3">Назначение ОКС</th>
            <th colspan="3">при наличии разводящих сетей ГВС после ЦТП</th>
            <th colspan="3">без разводящих ГВС</th>
        </tr>
        <tr>
            <th colspan="2">с изолированными стояками</th>
            <th>с неизолированными стояками</th>
            <th colspan="2">с изолированными стояками</th>
            <th>с неизолированными стояками</th>
        </tr>
        <tr>
            <th>без полотенцесушителей</th>
            <th>с полотенцесушителями</th>
            <th>с полотенцесушителями</th>
            <th>без полотенцесушителей</th>
            <th>с полотенцесушителями</th>
            <th>с полотенцесушителями</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var coef in Model)
        {
            <tr>
                <td>@coef.cpurp_type_name</td>

                @if(ViewBag.ConsumptionUnit == 1)
                {
                    <td><input type="text" class="data-value" value="@coef.result1_1m" disabled></td>
                    <td><input type="text" class="data-value" value="@coef.result2_1m" disabled></td>
                    <td><input type="text" class="data-value" value="@coef.result3_1m" disabled></td>
                    <td><input type="text" class="data-value" value="@coef.result4_1m" disabled></td>
                    <td><input type="text" class="data-value" value="@coef.result5_1m" disabled></td>
                    <td><input type="text" class="data-value" value="@coef.result6_1m" disabled></td>
                }
                @if (ViewBag.ConsumptionUnit == 2)
                {
                    <td><input type="text" class="data-value" value="@coef.result1_year" disabled></td>
                    <td><input type="text" class="data-value" value="@coef.result2_year" disabled></td>
                    <td><input type="text" class="data-value" value="@coef.result3_year" disabled></td>
                    <td><input type="text" class="data-value" value="@coef.result4_year" disabled></td>
                    <td><input type="text" class="data-value" value="@coef.result5_year" disabled></td>
                    <td><input type="text" class="data-value" value="@coef.result6_year" disabled></td>
                }
            </tr>
        }
    </tbody>
</table>