@using WebProject.Data
@model WebProject.Models.ObjectDescriptionViewModel

@{
    ViewData["Title"] = "УНОМЫ";
}

<style>
    .frst-label {
        width: 17%
    }
    .frst-label41 {
        width: 41%
    }
    .main-lbl {
        padding-bottom: 10px;
    }
    .panel-new {
        border: 1px solid transparent;
        border-radius: 10px;
        border-color: #ddd;
    }
    .col-md-6 {
        width: 100%;
    }
    .panel-grid {
        display: grid;
    }
    .form-group-grid {
        display: grid;
        width: 600px;
    }
    .form-group-block {
        width: 600px;
    }
    .div-flex {
        display: flex;
    }
    .lbl-font-12 {
        font-size: 12px; 
        padding-top: 0px !important;
    }
    .lbl-font-11 {
        font-size: 11px; 
        padding-top: 0px !important;
    }
    .inp-marg-top-15 {
        margin-top: 15px;
    }
    .inp-marg-top-30 {
        margin-top: 30px;
    }
    .inp-marg-top-45 {
        margin-top: 45px;
    }
    .input-group-w250 {
        width: 250px;
    }
    .bootstrap-select {
        width: 305px !important;
    }
    .col-md-6 {
        margin-bottom: inherit;
    }

    hr {
        display: block;
        height: 1px;
        border: 0;
        border-top: 1px solid #ccc;
        margin: 1em 0;
        padding: 0;
        margin-left: 15px;
    }
    .error {
        color: red;
    }
    .div-popup-success-mini {
        height: 150px;
        display: flex;
        align-items: center;
        justify-content: center;
        text-align: center;
        font-size: x-large;
    }

</style>

